  "description": "A transport for Pino that sends messages to Logflare",
  "homepage": "https://github.com/logflare/pino-logflare",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev:cli": "node ./test/debug.js | node ./src/cli.js",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "bin": {
